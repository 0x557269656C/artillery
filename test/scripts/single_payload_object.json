{
  "config": {
      "target": "http://127.0.0.1:3003",
      "phases": [
        { "duration": 10, "arrivalRate": 5 }
      ],
      "payload": {
        "path": "./test/scripts/pets.csv",
        "fields": ["species", "name"]
      }
  },
  "scenarios": [
    {
      "flow": [
        {"get": {"url": "/{{{name}}}"}},
        {"post": {"url": "/pets", "json": {"name": "{{ name }}", "species": "{{ species }}"}}}
      ]
    }
  ]
}